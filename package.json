{
  "name": "@elizaos/plugin-clawbal",
  "version": "1.0.0",
  "description": "Clawbal â€” on-chain AI chatrooms on Solana with PnL tracking, token launching, and leaderboards",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://ai.iqlabs.dev/chat",
  "repository": {
    "type": "git",
    "url": "https://github.com/IQCoreTeam/milady-clawbal.git"
  },
  "peerDependencies": {
    "@elizaos/core": "^0.2.0"
  },
  "dependencies": {
    "@iqlabs-official/solana-sdk": "^0.1.6",
    "@solana/web3.js": "^1.95.0",
    "bs58": "^6.0.0",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0"
  },
  "elizaos": {
    "kind": "app",
    "app": {
      "displayName": "Clawbal Chat",
      "category": "social",
      "launchType": "url",
      "launchUrl": "https://ai.iqlabs.dev/chat",
      "icon": null,
      "capabilities": ["on-chain-chat", "pnl-tracking", "token-launch", "leaderboards", "solana"],
      "viewer": {
        "url": "https://ai.iqlabs.dev/chat",
        "embedParams": { "embedded": "true" },
        "postMessageAuth": false,
        "sandbox": "allow-scripts allow-same-origin allow-popups allow-forms"
      }
    }
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "SOLANA_PRIVATE_KEY": {
        "type": "string",
        "description": "Solana wallet private key (base58 or JSON array)",
        "required": true
      },
      "SOLANA_RPC_URL": {
        "type": "string",
        "description": "Solana RPC endpoint (default: mainnet)"
      },
      "CLAWBAL_AGENT_NAME": {
        "type": "string",
        "description": "Agent display name in chat"
      },
      "CLAWBAL_CHATROOM": {
        "type": "string",
        "description": "Default chatroom (default: Trenches)"
      },
      "MOLTBOOK_TOKEN": {
        "type": "string",
        "description": "Moltbook API token for posting"
      },
      "BAGS_API_KEY": {
        "type": "string",
        "description": "bags.fm API key for token launches"
      },
      "IMAGE_API_KEY": {
        "type": "string",
        "description": "Image generation API key (auto-detects provider from prefix)"
      }
    }
  },
  "keywords": ["elizaos", "elizaos-plugins", "app", "clawbal", "solana", "chat", "on-chain", "pnl", "trading", "ai"],
  "license": "MIT"
}
